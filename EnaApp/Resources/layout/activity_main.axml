		/>
		<Button
			style="@style/LEDButtonStyle"
			android:text="Biombo"
			android:tag="biombo"
			android:id="@+id/buttonBiombo" />
		<Button
			style="@style/LEDButtonStyle"
			android:text="Bolama"
			android:tag="bolama"
			android:id="@+id/buttonBolama" />
		<Button
			style="@style/LEDButtonStyle"
			android:text="Cacheu"
			android:tag="cacheu"
			android:id="@+id/buttonCacheu" />
		<Button
			style="@style/LEDButtonStyle"
			android:text="Gabu"
			android:tag="gabu"
			android:id="@+id/buttonGabu" />
		<Button
			style="@style/LEDButtonStyle"
			android:text="Oio"
			android:tag="oio"
			android:id="@+id/buttonOio" />
		<Button
			style="@style/LEDButtonStyle"
			android:text="Quinara"
			android:tag="quinara"
			android:id="@+id/buttonQuinara" />
		<Button
			style="@style/LEDButtonStyle"
